{
  "name": "@instant-api/sport-session-tracker",
  "version": "1.0.1",
  "description": "> A CLI to create a Sport session tracker API",
  "homepage": "https://github.com/instant-api/sport-session-tracker#readme",
  "bugs": {
    "url": "https://github.com/instant-api/sport-session-tracker/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instant-api/sport-session-tracker.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc",
  "main": "dist/index.js",
  "module": "dist/sport-session-tracker.esm.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "sport-session-tracker": "bin/index.js"
  },
  "files": [
    "dist",
    "bin",
    "data",
    "README.md",
    "api.html"
  ],
  "scripts": {
    "build": "tsdx build",
    "lint": "tsdx lint",
    "pub": "tsdx build && np",
    "release": "yarn build && np",
    "run": "npx -p . sport-session-tracker --folder run-example",
    "run:slow": "npx -p . sport-session-tracker --folder run-example --slow",
    "script:fetch-places": "esr ./scripts/fetch-places.ts",
    "start": "tsdx watch",
    "test": "tsdx test --passWithNoTests",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "resolutions": {
    "**/typescript": "^4.2.3"
  },
  "dependencies": {
    "@faker-js/faker": "^6.0.0-alpha.5",
    "arg": "^5.0.1",
    "bcrypt": "^5.0.1",
    "better-sqlite3": "^7.5.0",
    "busboy": "^1.4.0",
    "cuid": "^2.1.8",
    "date-fns": "^2.28.0",
    "fs-extra": "^10.0.0",
    "get-port": "^5.1.1",
    "inquirer": "^8.2.0",
    "knex": "^1.0.1",
    "random": "^3.0.6",
    "tumau": "^1.0.0-alpha.95",
    "uid": "^2.0.0",
    "zod": "^3.11.6"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/busboy": "^1.3.0",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^27.4.0",
    "@types/node-fetch": "^2.5.8",
    "@types/yup": "^0.29.13",
    "cheerio": "^1.0.0-rc.10",
    "esbuild": "^0.14.16",
    "esbuild-runner": "^2.2.1",
    "node-fetch": "^2.6.1",
    "np": "^7.6.0",
    "prettier": "2.5.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public",
    "cache": "cache/.npm",
    "registry": "https://registry.npmjs.org"
  }
}
