{
  "name": "@instant-api/sport-session-tracker",
  "version": "1.0.1",
  "description": "> A CLI to create a Sport session tracker API",
  "homepage": "https://github.com/instant-api/sport-session-tracker#readme",
  "bugs": {
    "url": "https://github.com/instant-api/sport-session-tracker/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/instant-api/sport-session-tracker.git"
  },
  "license": "MIT",
  "author": "Etienne Dldc",
  "main": "dist/index.js",
  "module": "dist/sport-session-tracker.esm.js",
  "typings": "dist/index.d.ts",
  "bin": {
    "sport-session-tracker": "bin/index.js"
  },
  "files": [
    "dist",
    "bin",
    "data",
    "README.md",
    "api.html"
  ],
  "scripts": {
    "build": "tsdx build",
    "lint": "tsdx lint",
    "pub": "tsdx build && np",
    "run": "npx -p . sport-session-tracker --folder run-example",
    "run:slow": "npx -p . sport-session-tracker --folder run-example --slow",
    "start": "tsdx watch",
    "script:fetch-places": "esr ./scripts/fetch-places.ts",
    "test": "tsdx test --passWithNoTests",
    "typecheck": "tsc --noEmit"
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "arg": "^5.0.0",
    "bcrypt": "^5.0.1",
    "busboy": "^0.3.1",
    "cuid": "^2.1.8",
    "date-fns": "^2.19.0",
    "faker": "^5.5.2",
    "fs-extra": "^9.1.0",
    "get-port": "^5.1.1",
    "inquirer": "^8.0.0",
    "knex": "^0.95.4",
    "random": "^3.0.3",
    "sqlite3": "^5.0.2",
    "tumau": "1.0.0-alpha.85",
    "uid": "^2.0.0",
    "zod": "^3.0.0-alpha.33"
  },
  "devDependencies": {
    "@types/bcrypt": "^3.0.0",
    "@types/busboy": "^0.2.3",
    "@types/faker": "^5.5.0",
    "@types/fs-extra": "^9.0.8",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.20",
    "@types/node-fetch": "^2.5.8",
    "@types/yup": "^0.29.11",
    "cheerio": "^1.0.0-rc.5",
    "esbuild": "^0.11.2",
    "esbuild-runner": "^1.3.2",
    "node-fetch": "^2.6.1",
    "np": "^7.4.0",
    "prettier": "2.2.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "engines": {
    "node": ">=10"
  },
  "publishConfig": {
    "access": "public",
    "cache": "cache/.npm",
    "registry": "https://registry.npmjs.org"
  },
  "resolutions": {
    "**/typescript": "^4.2.3"
  }
}
